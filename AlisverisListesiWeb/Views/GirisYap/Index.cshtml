
@{
    Layout = null;
}

<
<!DOCTYPE html>
<html lang="en">
<head>
    <title>GİRİŞ YAP</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/GirisYap/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/GirisYap/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/GirisYap/css/main.css">
    <!--===============================================================================================-->
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100">
                <div class="login100-form-title" style="background-image: url(images/bg-01.jpg);">
                    <span class="login100-form-title-1">
                        GİRİŞ YAPINIZ
                    </span>
                </div>

                <form class="login100-form validate-form" action="~/GirisYap/Index/" method="post">
                    <div class="wrap-input100 validate-input m-b-26" data-validate="Username is required">
                        <span class="label-input100">Kullanıcı Adı</span>
                        <input class="input100" type="text" name="KullaniciAdi" placeholder="Kullanıcı Adı">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="wrap-input100 validate-input m-b-18" data-validate="Password is required">
                        <span class="label-input100">Şifre</span>
                        <input class="input100" type="password" name="Sifre" placeholder="Şifre">
                        <span class="focus-input100"></span>
                    </div>

                    <div class="flex-sb-m w-full p-b-30">
                        

                        <div>
                            <h2><a href="/Default/Index/" class="txt1">
                            ANA SAYFAYA DÖN
                            </a></h2>
                        </div>
                    </div>

                    <div class="container-login100-form-btn">
                        <button class="login100-form-btn">
                            GİRİŞ YAP
                        </button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/bootstrap/js/popper.js"></script>
    <script src="~/GirisYap/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/GirisYap/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/GirisYap/js/main.js"></script>

</body>
</html>